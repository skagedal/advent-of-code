{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Execute Command",
          "runtimeExecutable": "node",
          "runtimeArgs": ["--loader", "ts-node/esm", "--no-warnings=ExperimentalWarning"],
          "skipFiles": [
              "<node_internals>/**"
          ],
          "program": "${workspaceFolder}/bin/dev.js",
          "args": [
            "run"
          ],
          "internalConsoleOptions": "openOnSessionStart",
      }
  ]
}
